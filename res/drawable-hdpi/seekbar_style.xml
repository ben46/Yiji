<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="7.0dip" />
            <gradient android:startColor="#ffbababa" android:endColor="#ffebebeb" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffd5d5d5" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="7.0dip" />
                <gradient android:startColor="#80f82689" android:endColor="#a0ffcb00" android:angle="270.0" android:centerY="0.45" android:centerColor="#80ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="7.0dip" />
                <gradient android:startColor="#ff648d16" android:endColor="#ff9ac832" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff84b325" />
            </shape>
        </clip>
    </item>
</layer-list>
